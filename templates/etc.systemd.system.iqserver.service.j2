[Unit]
Description=iqserver service
After=network.target

[Service]
LimitNOFILE=65536
Type=forking
ExecStart={{ iqserver_base_dir }}/iqserver-{{ iqserver_version }}/bin/iqserver start
ExecStop={{ iqserver_base_dir }}/iqserver-{{ iqserver_version }}/bin/iqserver stop
User={{ iqserver_user }}
Restart=on-abort

[Install]
WantedBy=multi-user.target
